&НаСервере
Процедура Команда1НаСервере()
		Текст = новый ТекстовыйДокумент;
		Текст.Прочитать("E:\Новая папка\Спавочник1.csv");
		Для Стр=2 По Текст.КоличествоСтрок() Цикл
			Строка = СтрЗаменить(Текст.ПолучитьСтроку(Стр),";",Символы.ПС);
			Справочник = Справочники.Справочник1.СоздатьЭлемент();
			СтрокаБлок = СтрПолучитьСтроку(Строка,1);
			Справочник.Наименование = СтрокаБлок;
			если СтрНайти(СтрокаБлок,")")<>0 тогда 
				Справочник.Наименование = СтрЗаменить(СтрокаБлок,")",""); 
			КонецЕсли;
			Если СтрНайти(СтрокаБлок,"=")<>0 Тогда
			    Справочник.Наименование = СтрЗаменить(СтрокаБлок,"=","");
			КонецЕсли;
			
			Адрес = новый Картинка ("C:\Users\Public\Pictures\Sample Pictures\" + СтрПолучитьСтроку(Строка,2));
			Справочник.Фото = новый ХранилищеЗначения(Адрес);  
			  Справочник.Записать();

			КонецЦикла;

КонецПроцедуры
