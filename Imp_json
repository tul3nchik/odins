&НаСервере
Процедура Команда2НаСервере()	
Попытка
	ЧтениеJSON = Новый ЧтениеJSON;
		                                                                             	//Или из строки
		                                                                             	//ЧтениеJSON.УстановитьСтроку("");
		                                                                             	////Или из файла	
	ЧтениеJSON.ОткрытьФайл("E:\Новая папка\json.txt");
	Пока ЧтениеJSON.Прочитать() Цикл
		Если ЧтениеJSON.ТипТекущегоЗначения = ТипЗначенияJSON.НачалоМассива Тогда
			Пока ЧтениеJSON.Прочитать() Цикл
			                                                                             			//Если ЧтениеJSON.ТипТекущегоЗначения = ТипЗначенияJSON.Строка Тогда	
			                                                                             			//  	Сообщить("Значение: " + ЧтениеJSON.ТекущееЗначение);	
		                                                                             				//	
		                                                                              	//Иначе
				Если ЧтениеJSON.ТипТекущегоЗначения = ТипЗначенияJSON.НачалоОбъекта Тогда					
					Пока ЧтениеJSON.Прочитать() Цикл	
						Если ЧтениеJSON.ТипТекущегоЗначения = ТипЗначенияJSON.ИмяСвойства Тогда
							Если ЧтениеJSON.ТекущееЗначение = "Код" Тогда 
								Справочник=Справочники.Справочник1.СоздатьЭлемент();
							    	ЧтениеJSON.Прочитать();
								Справочник.Код =  ЧтениеJSON.ТекущееЗначение; 			
							КонецЕсли;
							Если ЧтениеJSON.ТекущееЗначение = "Наименование" Тогда
							    	ЧтениеJSON.Прочитать();
								Справочник.Наименование =  ЧтениеJSON.ТекущееЗначение;   
							КонецЕсли;
						КонецЕсли;
						Справочник.Записать();
					КонецЦикла;
				КонецЕсли;
			КонецЦикла;	
		КонецЕсли;	
	КонецЦикла;
	ЧтениеJSON.Закрыть();  
	Сообщить("Успешно импортировалось");
Исключение
Сообщить(ОписаниеОшибки());
КонецПопытки;
КонецПроцедуры
