Функция GETПолучить(Запрос)
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	Цветы.Код КАК Код,
		|	Цветы.Наименование КАК Наименование,
		|	Цветы.Статус КАК Статус,
		|	Цветы.Изготовитель КАК Изготовитель
		|ИЗ
		|	Справочник.Цветы КАК Цветы";
	
	РезультатЗапроса = Запрос.Выполнить();
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	ТабдЗнач = новый ТаблицаЗначений;
	ТабдЗнач.Колонки.Добавить("Код");
	ТабдЗнач.Колонки.Добавить("Наименование");
	ТабдЗнач.Колонки.Добавить("Статус");
	ТабдЗнач.Колонки.Добавить("Изготовитель");

	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		// Вставить обработку выборки ВыборкаДетальныеЗаписи
		Нов = ТабдЗнач.Добавить();
		Нов.Код = ВыборкаДетальныеЗаписи.Код;
		Нов.Наименование = ВыборкаДетальныеЗаписи.Наименование;
		Нов.Статус = ВыборкаДетальныеЗаписи.Статус;
		Нов.Изготовитель = ВыборкаДетальныеЗаписи.Изготовитель;

	КонецЦикла;
	
	Строка =  XMLСтрока(новый ХранилищеЗначения( ТабдЗнач, Новый СжатиеДанных(9)));
	//}}КОНСТРУКТОР_ЗАПРОСА_С_ОБРАБОТКОЙ_РЕЗУЛЬТАТА

	Ответ = Новый HTTPСервисОтвет(200);
	Ответ.УстановитьТелоИзСтроки(Строка);
	Возврат Ответ;
КонецФункции
